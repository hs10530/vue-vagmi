{
  "name": "vue-wagmi",
  "type": "module",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "vue",
    "hooks",
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3"
  ],
  "homepage": "https://github.com/wobsoriano/vue-wagmi#readme",
  "bugs": {
    "url": "https://github.com/wobsoriano/vue-wagmi/issues"
  },
  "license": "MIT",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-wagmi.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esmo src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "ethers": ">=5.5.1",
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.0.11",
    "@vueuse/core": "^8.4.2",
    "@wagmi/core": "^0.2.4",
    "@walletconnect/ethereum-provider": "^1.7.8",
    "klona": "^2.0.5",
    "vue-query": "^1.23.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.16.2",
    "@types/node": "^17.0.31",
    "bumpp": "^7.1.1",
    "esmo": "^0.14.1",
    "ethers": "^5.6.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vitest": "^0.10.2",
    "vue": "^3.2.33"
  }
}
